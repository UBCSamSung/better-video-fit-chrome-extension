{
    "name": "Better Video Fit",
    "version": "1.0",
    "description": "Chrome extension that toggle video aspect ratio between cover, fill, none, and default",
    "permissions": [
        "activeTab",
        "storage",
        "alarms"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "name": "Click to change object-fit"
    },
    "icons": {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "192": "icon-192.png",
        "512": "icon-512.png"
      },
    "manifest_version": 2
}